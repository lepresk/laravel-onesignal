# OneSignal Configuration
ONESIGNAL_APP_ID=your-app-id-here
ONESIGNAL_REST_API_KEY=your-rest-api-key-here
ONESIGNAL_USER_AUTH_KEY=
ONESIGNAL_ANDROID_CHANNEL_ID=your-android-channel-id

# Default Settings
ONESIGNAL_DEFAULT_TTL=604800
ONESIGNAL_DEFAULT_PRIORITY=5

# Logging
ONESIGNAL_LOGGING_ENABLED=true
ONESIGNAL_LOG_CHANNEL=
ONESIGNAL_LOG_LEVEL=info

# Exception Handling
ONESIGNAL_THROW_EXCEPTIONS=true

# HTTP Client
ONESIGNAL_HTTP_TIMEOUT=30
ONESIGNAL_RETRY_TIMES=3
ONESIGNAL_RETRY_SLEEP=1000
